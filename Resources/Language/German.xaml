    <system:String x:Key="WrongAuth">Falsches Passwort oder Username, registriere dich auf {0}.</system:String>
    <system:String x:Key="AuthEmpty">Passwort oder Username ist nicht ausgefüllt.</system:String>
    <system:String x:Key="FailedToDelete">Die Aktualisierungdatei konnte nicht gelöscht werden, lösche sie manuell und starte LeagueSharp neu.</system:String>
    <system:String x:Key="WrongVersion">Deine League of Legends Version wird nicht unterstützt, Hash:</system:String>
    <system:String x:Key="FailedToDownload">Download ist gescheitert.</system:String>
    <system:String x:Key="FailedToLogin">Anmeldung gescheitert: {0}</system:String>
    <system:String x:Key="Login">Anmelden</system:String>
    <system:String x:Key="LoginError">Anmeldung als {0}: {1} gescheitert</system:String>
    <system:String x:Key="Uninstall">Deinstalliere</system:String>
    <system:String x:Key="UninstallConfirm">Bist du dir sicher, dass du die ausgewählte Assembly löschen willst?</system:String>
    <system:String x:Key="ConfigWriteError">Konnte nicht in die config.xml Datei schreiben.</system:String>
    <system:String x:Key="ShareText">Assembly link wurde in den Zwischenspeicher gelegt.</system:String>
    <system:String x:Key="LogNotFound">Logdatei wurde nicht gefunden.</system:String>
    <system:String x:Key="NewProfile2">Neues Profil</system:String>
    <system:String x:Key="Rename">Umbennen</system:String>
    <system:String x:Key="RenameText">Gebe den neuen Profilnamen ein.</system:String>
    <system:String x:Key="DownloadingData">Lade die benötigten Datein runter.</system:String>
    <system:String x:Key="Installer">Installierer</system:String>
    <system:String x:Key="SuccessfullyInstalled">Ausgewählte Assembly wurde erfolgreich installiert.</system:String>
    <system:String x:Key="ErrorInstalling">Es gab einen Fehler beim installieren der Assemblies, bitte überprüfte die Logdatei.</system:String>

</ResourceDictionary>