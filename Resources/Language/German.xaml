    <system:String x:Key="Previous">Vorherig</system:String>
    <system:String x:Key="Add">Hinzufügen</system:String>

    <!-- Settings General -->
    <system:String x:Key="General">Allgemein</system:String>
    <system:String x:Key="GameSettings">Spieleinstellungen</system:String>
    <system:String x:Key="Anti-AFK">Anti-AFK</system:String>
