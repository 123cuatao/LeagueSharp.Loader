    <system:String x:Key="NewProfile">Neu</system:String>
    <system:String x:Key="ChangeName">Name ändern</system:String>
    <system:String x:Key="DeleteProfile">Löschen</system:String>

    <!-- MainWindow Buttons -->
    <system:String x:Key="UpdateAll">Aktualisiere alles</system:String>
    <system:String x:Key="CompileAll">Erstelle alles</system:String>
    <system:String x:Key="InstallNewAssembly">Installiere eine neue Assembly</system:String>

    <!-- InstallerWindow Title -->
    <system:String x:Key="AssemblyInstaller">Assembly Installierer</system:String>

    <!-- InstallerWindow Tabs -->
    <system:String x:Key="Search">Suche</system:String>
    <system:String x:Key="Select">Auswählen</system:String>

    <!-- InstallerWindow Search tab -->
    <system:String x:Key="SelectAssembliesText">Wähle den Standort der Assembly:</system:String>
    <system:String x:Key="Local">Lokal</system:String>
    <system:String x:Key="Github">GitHub</system:String>
    <system:String x:Key="Installed">Installiert</system:String>
    <system:String x:Key="Next">Weiter</system:String>

    <!-- InstallerWindow Select tab -->
    <system:String x:Key="SelectAll">Wähle alles aus</system:String>
