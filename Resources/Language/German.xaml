<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!-- MainWindow TopBar -->
    <system:String x:Key="Website">Webseite</system:String>
    <system:String x:Key="Settings">Einstellungen</system:String>

    <!-- MainWindow Tabs -->
    <system:String x:Key="News">Neuigkeiten</system:String>
    <system:String x:Key="Assemblies">Assemblies</system:String>

    <!-- MainWindow Profile selector -->
    <system:String x:Key="DefaultProfile">Default</system:String>

    <!-- MainWindow Assemblies datagrid columns -->
    <system:String x:Key="ColumnName">Name</system:String>
    <system:String x:Key="ColumnType">Art</system:String>
    <system:String x:Key="ColumnVersion">Version</system:String>
    <system:String x:Key="ColumnLocation">Standort</system:String>
    <system:String x:Key="ColumnDescription">Beschreibung</system:String>

    <!-- MainWindow Assemblies datagrid Menu -->
    <system:String x:Key="MenuUpdateAndCompile">Aktualisieren und erstellen</system:String>
    <system:String x:Key="MenuRemove">Entfernen</system:String>
    <system:String x:Key="MenuGitub">GitHub</system:String>
    <system:String x:Key="MenuShare">Teilen</system:String>
    <system:String x:Key="MenuViewLog">Logs ansehen</system:String>

