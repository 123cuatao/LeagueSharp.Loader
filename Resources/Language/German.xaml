    <system:String x:Key="Reload the assemblies">Lade die Assemblies neu</system:String>
    <system:String x:Key="Recompile and reload the assemblies">Erstelle und lade die Assemblies neu</system:String>
    <system:String x:Key="Shows the menu (Press)">Zeige das Menü an (Press)</system:String>
    <system:String x:Key="Shows the menu (Toggle)">Zeige das Menü an (Toggle)</system:String>

    <!-- Settings Log -->
    <system:String x:Key="Log">Log</system:String>
    <system:String x:Key="Status">Status</system:String>
    <system:String x:Key="Source">Quelle</system:String>
    <system:String x:Key="Content">Inhalt</system:String>
    <system:String x:Key="OpenFolder">Öffne den Ordner</system:String>

